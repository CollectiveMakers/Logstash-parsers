# W3C logs format with all fields
IIS_W3C %{TIMESTAMP_ISO8601:time_logged} %{IPORHOST:site_name} %{HOSTNAME:dst_host} (?:%{IPV4:dst_ip}|%{IPV6:dst_ipv6}) %{WORD:http_method} (-|%{URIPATH:uri}) (-|%{NOTSPACE:query_string}) %{NUMBER:dst_port} (-|%{NOTSPACE:user_name}) (?:%{IPV4:src_ip}|%{IPV6:src_ipv6}|%{HOSTNAME:src_host}) (%{DATA:http_version}|-) (-|%{NOTSPACE:user_agent}) (%{DATA:cookie}|-) (-|%{DATA:referer}) %{HOSTNAME:hostname} %{NUMBER:http_status} %{NUMBER:substatus:int} %{NUMBER:win32_status:int} %{NUMBER:bytes_received:int} %{NUMBER:bytes_sent:int} %{NUMBER:duration:int}
